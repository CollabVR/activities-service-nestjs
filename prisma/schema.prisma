generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Activity {
  id              Int    @id @default(autoincrement())
  name            String @unique
  description     String
  startTime       String
  endTime         String
  maxParticipants Int
  participants    Int[]
  moderators      Int[]
  environmentId   Int
  status          String
}
